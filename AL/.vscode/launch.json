{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "BC18_AGFW WebClient",
            "request": "launch",
            "type": "al",
            "environmentType": "OnPrem",
            "server": "http://navdev1:8080",
            "serverInstance": "BC18_AGFW",
