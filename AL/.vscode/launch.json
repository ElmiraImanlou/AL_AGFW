        {
            "name": "BC18_AGFW Background",
            "request": "attach",
            "type": "al",
            "server": "http://navdev1:8080",
            "serverInstance": "BC18_AGFW",
            "port": 18029,
            "authentication": "Windows",
            "breakOnError": true,
